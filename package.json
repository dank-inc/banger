{
  "name": "@dank-inc/banger",
  "version": "0.11.8",
  "author": "Elijah Lucian",
  "description": "Typescript Sound Banger",
  "repository": {
    "type": "git",
    "url": "https://github.com/dank-inc/banger"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "license": "MIT",
  "keywords": [
    "typescript",
    "webaudio",
    "sound",
    "audio",
    "sfx",
    "bgm",
    "sound player"
  ],
  "scripts": {
    "dev": "esbuild ./src/index.ts --serve=1234 --outdir=./public --servedir=./public --bundle",
    "lint": "tsc --noEmit",
    "compile": "rm -rf dist && tsc --emitDeclarationOnly --outDir dist && npm run compile:mini",
    "compile:mini": "esbuild ./src/index.ts --outdir=./dist --bundle --minify",
    "deploy": "npm run compile && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "esbuild": "^0.14.53"
  },
  "dependencies": {
    "@dank-inc/numbaz": "^0.3.0"
  }
}
